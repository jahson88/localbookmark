    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.3",
    "buble": "^0.19.8",
    "buble-loader": "^0.5.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
